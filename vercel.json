{
  "name": "botform",
  "version": "1.0.0",
  "description": "Aplicação web para executar scripts Playwright com autenticação Supabase",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".",
  "framework": "express",
  "regions": ["sfo1"],
  "env": [
    {
      "key": "SUPABASE_URL",
      "description": "URL do seu projeto Supabase"
    },
    {
      "key": "SUPABASE_ANON_KEY",
      "description": "Chave anônima do Supabase"
    },
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "PORT",
      "value": "3000"
    }
  ]
}
